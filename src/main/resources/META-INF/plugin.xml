<idea-plugin>
    <id>com.zlf.sql-reverse.plugin</id>
    <name>SqlReverse</name>
    <version>1.0</version>
    <vendor email="704123055@qq.com">zhanglinfeng</vendor>
    <description><![CDATA[
      定制化代码生成器，根据建表SQL一键生成mapper、model、VO、service、controller
      包括以下功能</br>
      1.基础增删改查、列表的查询
    ]]></description>
    <change-notes><![CDATA[
      1.0 测试版
    ]]></change-notes>
    <idea-version since-build="173.0"/>
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- 窗体 (IDEA 界面右侧) -->
        <toolWindow id="Paste SQL" secondary="false" anchor="right" icon="/icon/rightTool.png" factoryClass="factory.ReadFactory"/>
    </extensions>
</idea-plugin>